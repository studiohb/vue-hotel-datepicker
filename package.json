{
  "name": "vue-hotel-datepicker",
  "version": "3.1.14",
  "description": "A responsive date range picker for Vue.js that displays the number of nights selected and allow several useful options like custom check-in/check-out rules, localization support and more",
  "author": "krystalcampioni <hello@krystalcampioni.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --report-json --target lib --filename vue-hotel-datepicker",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "fecha": "^3.0.3",
    "lodash.throttle": "^4.1.1",
    "vue": "^2.6.11",
    "vue-on-click-outside": "^1.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^5.2.3",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "main": "dist/vue-hotel-datepicker.umd.min.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/studiohb/vue-hotel-datepicker"
  },
  "keywords": [
    "vue",
    "vuejs",
    "date",
    "dates",
    "input",
    "date input",
    "datepicker",
    "date picker",
    "range",
    "date range picker"
  ],
  "bugs": {
    "url": "https://github.com/studiohb/vue-hotel-datepicker/issues"
  },
  "homepage": "https://github.com/studiohb/vue-hotel-datepicker"
}
